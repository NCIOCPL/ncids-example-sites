{
	"name": "ncids-webpack",
	"version": "1.0.0",
	"description": "NCIDS example repository using webpack",
	"main": "index.js",
	"keywords": [
		"National Cancer Institute Design System",
		"ncids",
		"webpack",
		"html",
		"sass",
		"javascript"
	],
	"author": "National Cancer Institute",
	"license": "ISC",
	"scripts": {
		"build:dev": "webpack --mode development",
		"build:prod": "webpack --mode production",
		"build:watch": "webpack watch --mode development",
		"build:ncids-css": "sass -I ./node_modules/@nciocpl/ncids-css/uswds-packages -I ./node_modules/@nciocpl/ncids-css/packages ./node_modules/@nciocpl/ncids-css/index.scss ./dist/ncids-compiled.css",
		"build:app-css": "sass -I ./node_modules/@nciocpl/ncids-css/uswds-packages -I ./node_modules/@nciocpl/ncids-css/packages ./src/scss/index.scss ./dist/app-style.css"
	},
	"devDependencies": {
		"autoprefixer": "^10.3.7",
		"css-loader": "^6.5.1",
		"css-minimizer-webpack-plugin": "^3.4.1",
		"html-webpack-plugin": "^5.5.0",
		"mini-css-extract-plugin": "^2.6.0",
		"path": "^0.12.7",
		"postcss-loader": "^4.3.0",
		"sass": "^1.71.0",
		"sass-loader": "^10.1.1",
		"webpack": "^5.70.0",
		"webpack-cli": "^4.9.2"
	},
	"dependencies": {
		"@nciocpl/ncids-css": "^3.0.0",
		"@nciocpl/ncids-js": "^3.0.0"
	},
	"browserslist": {
		"production": [
			">1%",
			"last 2 versions",
			"not dead",
			"not IE 11"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
